provider "yoyo"
name "adservers"
fetch_http('http://pgl.yoyo.org/adservers/serverlist.php?hostformat=nohtml')

filter_whitespace
filter_comments

parse_eachline(:separator => "\n") do |event_generator, record|
  domain = record.data.strip()
  next if domain.nil?

  event_generator.call() do |event|
    event.type = :scanning
    event.add_fqdn(domain)
  end
end
